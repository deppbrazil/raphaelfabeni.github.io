# editorconfig.org
root = false

# All files
[*]
indent_style = tab
indent_size = 2
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

# HTML
[*.html]
indent_style = tab
indent_size = 2

# CSS
[*.css]
indent_style = tab
indent_size = 2

# JS
[*.js]
indent_style = tab
indent_size = 2

# md
[*.md]
trim_trailing_whitespace = false
